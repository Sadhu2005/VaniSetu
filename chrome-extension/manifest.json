{
  "manifest_version": 3,
  "name": "ANU - Voice Dub AI",
  "version": "1.0",
  "description": "AI-powered video dubbing.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "*://*.youtube.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}